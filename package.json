{
  "name": "moc-project",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "sequalize:migrate:make": "./node_modules/.bin/sequelize --migrations-path=./src/db/sequelize/migrations migration:generate --name",
    "sequalize:migrate:latest": "./node_modules/.bin/sequelize --migrations-path=./src/db/sequelize/migrations/ --config=./src/db/sequelize/config/config.js db:migrate",
    "sequalize:migrate:undo": "./node_modules/.bin/sequelize --migrations-path=./src/db/sequelize/migrations/ --config=./src/db/sequelize/config/config.js db:migrate:undo",
    "knex:migrate:make": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js migrate:make",
    "knex:migrate:latest": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js  migrate:latest",
    "knex:migrate:up": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js  migrate:up",
    "knex:migrate:down": "./node_modules/.bin/knex --knexfile=./src/db/knex/knexfile.js  migrate:down",
    "pg:migrate:make": "./node_modules/.bin/node-pg-migrate --migrations-dir=./src/db/pg/migrations/ --config-file=./src/db/pg/config/index.js create",
    "pg:migrate:up": "./node_modules/.bin/node-pg-migrate --migrations-dir=./src/db/pg/migrations/ --config-file=./src/db/pg/config/index.js up",
    "pg:migrate:down": "./node_modules/.bin/node-pg-migrate --migrations-dir=./src/db/pg/migrations/ --config-file=./src/db/pg/config/index.js down",
    "pg:migrate:redo": "./node_modules/.bin/node-pg-migrate --migrations-dir=./src/db/pg/migrations/ --config-file=./src/db/pg/config/index.js redo"
  },
  "author": "Vladyslav Kravets",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-basic-auth": "^1.2.0",
    "knex": "^0.21.13",
    "nanoid": "^3.1.18",
    "node-pg-migrate": "^5.9.0",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  }
}
